@model System.Web.Mvc.HandleErrorInfo


@{
    Layout = "~/Views/Shared/_LayoutModal.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Model.Exception</title>
</head>
<body>
    <div>
        <h4 class="text-danger">@Model.Exception.Message</h4>
        </div>

        <div>
            <h4 class="text-danger">Clique no botão para reiniciar processo</h4>
        </div>

    <div>
        @Html.ActionLink("BUSCAR LISTA", "Index", "Projeto", null, new { @class = "btn btn-primary" })
    </div>
</body>
</html>


